{
  "name": "@kodo/ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./globals.css": "./dist/globals.css"
  },
  "scripts": {
    "build": "tsup && tailwindcss -i ./src/globals.css -o ./dist/globals.css --minify",
    "dev": "concurrently \"tsup --watch\" \"tailwindcss -i ./src/globals.css -o ./dist/globals.css --watch\"",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .turbo"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
