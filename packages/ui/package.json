    "react-hook-form": "^7.63.0",
    "react-resizable-panels": "^2.1.9",
    "recharts": "2.15.4",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.14",
    "@types/react": "catalog:",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.14",
    "tsup": "^8.0.0",
    "tw-animate-css": "1.3.3",
    "typescript": "catalog:"
  }
